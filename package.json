{
  "name": "vort",
  "description": "Flexible backend framework",
  "version": "0.1.5",
  "author": "kotsmile",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/kotsmile/vort"
  },
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "dev": "rm -rf ./dist && tsc && tsc-alias && node ./dist/example-backend/index.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "tsc --noEmit && prettier --check \"./src/**/*.{js,ts,jsx,tsx,vue}\"",
    "format": "prettier --write \"./src/**/*.{js,ts,jsx,tsx,vue}\"",
    "build": "rm -rf ./lib && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json"
  },
  "keywords": [
    "express",
    "backend",
    "typescript"
  ],
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/swagger-ui-express": "^4.1.3",
    "swagger-ui-express": "^4.6.2",
    "zod-to-json-schema": "^3.20.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@types/jest": "^29.5.0",
    "chai": "^4.3.7",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.5",
    "typescript": "^5.0.4",
    "zod": "^3.21.4",
    "express": "^4.18.2"
  },
  "peerDependencies": {
    "zod": "^3.21.4",
    "express": "^4.18.2"
  }
}
